<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hustascii.torrentdemo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="21" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>
    

    <application
        android:allowBackup="true"
       android:icon="@drawable/mioseed"
        android:label="@string/title"
       
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Welcome"
            android:label="@string/title" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
         <activity
            android:name=".MainActivity"
            android:label="@string/title" >
           
        </activity>
         <activity
            android:name="com.hustascii.torrentdemo.activitys.ResultActivity"
             android:windowSoftInputMode="stateHidden"
            android:label="@string/app_name" >
            
        </activity>
        <activity
            android:name="com.hustascii.torrentdemo.activitys.DetailActivity"
            android:label="@string/app_name" >
            
        </activity>
         <!-- <activity
            android:name="com.hustascii.torrentdemo.activitys.FileExplorerActivity"
            android:label="@string/app_name" >
            
        </activity> -->
       
    </application>

</manifest>
